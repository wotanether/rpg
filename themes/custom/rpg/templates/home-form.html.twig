 * @see template_preprocess_form()
 */
#}

<h2 class="home-form-title">Welcome to </br>I don't have a title yet...</h2>
<div class="row">
    <div class="small-6 large-6 large-offset-3 small-offset-3 columns">
        {{ form.new_game }}
    </div>
</div>
<div class="row">
    <div class="form-home small-6 large-6 large-offset-3 small-offset-3 columns">
        {{ form.continue }}
    </div>
</div>
{{ form|without('new_game', 'continue') }}
